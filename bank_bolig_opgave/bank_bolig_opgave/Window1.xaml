<Window x:Class="bank_bolig_opgave.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:bank_bolig_opgave"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        
        <TextBox HorizontalAlignment="Left" Margin="208,206,0,0" TextWrapping="Wrap" Text="{Binding MyEarnings, Converter={StaticResource myEarnings},UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="217" Height="92"/>
    </Grid>
</Window>
